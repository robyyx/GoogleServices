<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hu.aar.FoMenu">
    <children>
        <fx:include source="Navigator.fxml" />
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="buttonSaveDocument" layoutX="146.0" layoutY="336.0" mnemonicParsing="false" onAction="#saveDocumentToRootFolder" text="Mentés" />
            <TextArea fx:id="textAreaDocumentContent" layoutX="25.0" layoutY="51.0" prefHeight="276.0" prefWidth="588.0" />
            <Text layoutX="25.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Név:" />
            <TextField fx:id="textFieldDocumentName" layoutX="53.0" layoutY="22.0" />
            <Button layoutX="215.0" layoutY="336.0" mnemonicParsing="false" text="Mentés másként" />
            <Button fx:id="buttonOpenDocuments" layoutX="333.0" layoutY="336.0" mnemonicParsing="false" onAction="#loadDocument" text="Megnyitás" />
            <Button layoutX="419.0" layoutY="336.0" mnemonicParsing="false" text="Megosztás" />
            <Button fx:id="buttonNewDocument" layoutX="27.0" layoutY="336.0" mnemonicParsing="false" onAction="#clearDocument" prefHeight="25.0" prefWidth="104.0" text="Új dokumentum" />
         </children>
        </AnchorPane>
    </children>
</VBox>
